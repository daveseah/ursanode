{
  "name": "root",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:daveseah/ursanode.git"
  },
  "scripts": {
    "bootstrap": "lerna bootstrap --hoist",
    "rebootstrap": "lerna clean -y && lerna bootstrap --hoist",
    "audit": "lerna rebootstrap && lerna-audit",
    "build": "lerna run build",
    "start": "npm run build && multiview [npm run ex-next]",
    "ursys": "#ERR 'ursys is not runnable from lerna root'",
    "ex-module": "cd ur_packages/ex-module && npm run local",
    "ex-next": "cd ur_packages/ex-next && npm run local",
    "ex-html": "cd ur_packages/ex-html-js && npm run server",
    "taskeroids": "cd ur_packages/ex-taskeroids && npm run server"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "lerna": "^3.20.2",
    "lerna-audit": "^1.2.0",
    "lerna-update-wizard": "^0.17.8",
    "prettier": "^2.0.5",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-loader": "^8.1.0",
    "fs-extra": "^8.1.0",
    "hashids": "^2.2.1",
    "ip": "^1.1.5",
    "minimist": "^1.2.5",
    "request-ip": "^2.1.3",
    "shelljs": "^0.8.4",
    "tracer": "^1.0.3",
    "ts-loader": "^7.0.4"
  }
}
